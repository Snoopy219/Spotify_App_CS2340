<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginHorizontal="8dp"
    android:layout_marginVertical="16dp"
    app:cardCornerRadius="8dp"
    app:cardElevation="10dp">

    <LinearLayout
        android:layout_width="192dp"
        android:layout_height="320dp"
        android:background="@color/tinted_white"
        android:orientation="vertical"
        android:padding="8dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="303dp"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="8dp">

            <TextView
                android:id="@+id/dateTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_bold"
                android:text="Date"
                android:textColor="@color/seafoam_blue"
                android:textSize="26sp" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_semibold"
                android:text="Top Songs"
                android:textColor="@color/seafoam_blue"
                android:textSize="15sp"
                tools:layout_editor_absoluteX="99dp"
                tools:layout_editor_absoluteY="140dp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_semibold"
                android:text="Top Genre"
                android:textColor="@color/seafoam_blue"
                android:textSize="15sp"
                tools:layout_editor_absoluteX="52dp"
                tools:layout_editor_absoluteY="243dp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="97dp"
                android:fontFamily="@font/montserrat_semibold"
                android:text="Top Artists"
                android:textColor="@color/seafoam_blue"
                android:textSize="15sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView4" />

            <TextView
                android:id="@+id/artist1TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1."
                android:textSize="10sp"
                tools:layout_editor_absoluteX="1dp"
                tools:layout_editor_absoluteY="164dp" />

            <TextView
                android:id="@+id/artist2TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2."
                android:textSize="10sp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="178dp" />

            <TextView
                android:id="@+id/artist3TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="3."
                android:textSize="10sp"
                tools:layout_editor_absoluteX="1dp"
                tools:layout_editor_absoluteY="192dp" />

            <TextView
                android:id="@+id/artist4TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="4."
                android:textSize="10sp"
                tools:layout_editor_absoluteX="1dp"
                tools:layout_editor_absoluteY="206dp" />

            <TextView
                android:id="@+id/artist5TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="5."
                android:textSize="10sp"
                tools:layout_editor_absoluteX="1dp"
                tools:layout_editor_absoluteY="220dp" />

            <TextView
                android:id="@+id/song1TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1."
                android:textSize="10sp"
                tools:layout_editor_absoluteX="99dp"
                tools:layout_editor_absoluteY="164dp" />

            <TextView
                android:id="@+id/song2TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2."
                android:textSize="10sp"
                tools:layout_editor_absoluteX="99dp"
                tools:layout_editor_absoluteY="178dp" />

            <TextView
                android:id="@+id/song3TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="3."
                android:textSize="10sp"
                tools:layout_editor_absoluteX="99dp"
                tools:layout_editor_absoluteY="192dp" />

            <TextView
                android:id="@+id/song4TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="4."
                android:textSize="10sp"
                tools:layout_editor_absoluteX="99dp"
                tools:layout_editor_absoluteY="206dp" />

            <TextView
                android:id="@+id/song5TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="5."
                android:textSize="10sp"
                tools:layout_editor_absoluteX="99dp"
                tools:layout_editor_absoluteY="220dp" />

            <TextView
                android:id="@+id/topGenreTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="blank"
                android:textSize="10sp"
                tools:layout_editor_absoluteX="68dp"
                tools:layout_editor_absoluteY="267dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>

</LinearLayout>